{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python",
      "config": {
        "installCommand": "pip install -r requirements.txt"
      }
    }
  ],
  "routes": [
    {
      "src": "/api",
      "dest": "main.py"
    },
    {
      "src": "/apis",
      "dest": "app.py"
    }
  ]
}
